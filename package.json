{
  "name": "contentful-table-ui",
  "version": "1.0.0",
  "description": "Contentful UI Extension to display JSON as Tables",
  "main": "index.js",
  "engines": {
    "node": ">=7.6"
  },
  "scripts": {
    "create": "contentful extension create  --management-token ${CONTENTFUL_MANAGEMENT_ACCESS_TOKEN} --space-id ${SPACE_ID} --environment-id ${ENVIRONMENT_ID} --descriptor ./extension.json",
    "update": "contentful extension update  --management-token ${CONTENTFUL_MANAGEMENT_ACCESS_TOKEN} --space-id ${SPACE_ID} --environment-id ${ENVIRONMENT_ID} --descriptor ./extension.json --force"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ljvmiranda921/ui-editable-table.git"
  },
  "keywords": [
    "contentful"
  ],
  "author": "stefan judis <stefanjudis@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ljvmiranda921/ui-editable-table/issues"
  },
  "homepage": "https://github.com/ljvmiranda921/ui-editable-table#readme",
  "dependencies": {
    "contentful-cli": "^1.12.33"
  }
}
